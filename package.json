{
  "name": "wc3multitool",
  "version": "2.0.5-alpha",
  "description": "A multi-tool for Warcraft 3",
  "repository": {
    "type": "git",
    "url": "git://github.com/kgallimore/wc3multitool.git"
  },
  "author": "Trenchguns",
  "license": "UNLICENSED",
  "main": "packages/main/dist/index.cjs",
  "scripts": {
    "build": "npm run build:main && npm run build:preload && npm run build:renderer",
    "build:main": "cd ./packages/main && vite build",
    "build:preload": "cd ./packages/preload && vite build",
    "build:renderer": "cd ./packages/renderer && vite build",
    "compile": "cross-env MODE=production npm run build && electron-builder build --config .electron-builder.config.js --dir --config.asar=false",
    "test": "npm run test:main && npm run test:preload && npm run test:renderer && npm run test:e2e",
    "test:e2e": "npm run build && vitest run",
    "test:main": "vitest run -r packages/main --passWithNoTests",
    "test:preload": "vitest run -r packages/preload --passWithNoTests",
    "test:renderer": "vitest run -r packages/renderer --passWithNoTests",
    "watch": "node scripts/watch.mjs",
    "lint": "eslint . --ext js,mjs,cjs,ts,mts,cts,svelte",
    "typecheck:main": "tsc --noEmit -p packages/main/tsconfig.json",
    "typecheck:preload": "tsc --noEmit -p packages/preload/tsconfig.json",
    "typecheck:renderer": "tsc --noEmit -p packages/renderer/tsconfig.json",
    "typecheck": "npm run typecheck:main && npm run typecheck:preload && npm run typecheck:renderer",
    "postinstall": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/update-electron-vendors.mjs",
    "format": "npx prettier --write \"**/*.{js,mjs,cjs,ts,mts,cts,svelte,json}\""
  },
  "devDependencies": {
    "@svelte-plugins/tooltips": "^0.1.6",
    "@tsconfig/svelte": "^5.0.0",
    "@types/audio-play": "^2.3.0",
    "@types/discord-rpc": "^4.0.5",
    "@types/node": "18.17.0",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "@zerodevx/svelte-toast": "^0.9.5",
    "autoprefixer": "^10.4.14",
    "cross-env": "7.0.3",
    "electron": "25.3.1",
    "electron-builder": "24.6.3",
    "eslint": "^8.45.0",
    "eslint-plugin-svelte": "^2.32.4",
    "happy-dom": "10.5.2",
    "playwright": "1.36.1",
    "postcss": "^8.4.27",
    "svelte": "^4.1.1",
    "tailwindcss": "^3.3.3",
    "typescript": "5.1.6",
    "unplugin-auto-expose": "0.0.5",
    "vite": "4.4.7",
    "vitest": "0.33.0"
  },
  "dependencies": {
    "@discordjs/rest": "^1.7.1",
    "@iamtraction/google-translate": "^2.0.1",
    "@nut-tree/nl-matcher": "^2.2.0",
    "@nut-tree/nut-js": "^3.1.2",
    "@prisma/client": "^5.0.0",
    "audio-play": "^2.3.1",
    "bufferutil": "^4.0.7",
    "discord-api-types": "^0.37.50",
    "discord-rpc": "^4.0.1",
    "discord.js": "^14.11.0",
    "electron-log": "^5.0.0-beta.25",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.1.1",
    "fast-geoip": "^1.1.88",
    "js-combinatorics": "^2.1.1",
    "languagedetect": "^2.0.0",
    "mariadb": "^3.2.0",
    "mysql2": "^3.5.2",
    "nanoid": "^4.0.2",
    "node-abi": "^3.45.0",
    "node-window-manager": "^2.2.4",
    "obs-websocket-js": "^5.0.3",
    "prisma": "^5.0.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.32.1",
    "sequelize-typescript": "^2.1.5",
    "socket.io-client": "^2.5.0",
    "sqlite3": "^5.1.6",
    "translate": "^2.0.2",
    "w3gjs": "^2.5.0",
    "wc3mt-lobby-container": "^1.1.1",
    "ws": "^8.13.0"
  }
}
